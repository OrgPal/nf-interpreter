{
    "version": 4,
    "include": [
        "../../../CMake/arm-gcc.json",
        "../../../config/user-tools-repos.json",
        "../../../config/user-prefs.json"
    ],    
    "configurePresets": [
        {
            "name": "ORGPAL_RADAR",
            "inherits": [
                "arm-gcc-cortex-preset",
                "user-tools-repos",
                "user-prefs"
            ],
            "hidden": false,
            "cacheVariables": {
                "TARGET_BOARD": "ORGPAL_RADAR",
                "TARGET_SERIES": "STM32F7xx",
                "RTOS": "ChibiOS",
                "BUILD_VERBOSE": "OFF",
                "CHIBIOS_CONTRIB_REQUIRED": "OFF",
                "SUPPORT_ANY_BASE_CONVERSION": "ON",
                "NF_FEATURE_DEBUGGER": "ON",
                "NF_FEATURE_RTC": "ON",
                "NF_FEATURE_HAS_CONFIG_BLOCK": "OFF",
                "NF_BUILD_RTM": "OFF",
                "API_System.Math": "OFF",
                "API_Hardware.Stm32": "OFF",
                "API_System.Device.Gpio": "ON",
                "API_System.Device.Spi": "ON",
                "API_System.Device.I2c": "OFF",
                "API_System.IO.Ports": "ON",
                "API_System.Device.Adc": "OFF",
                "API_System.Device.Dac": "ON",
                "API_nanoFramework.ResourceManager": "OFF",
                "API_nanoFramework.System.Collections": "ON",
                "API_nanoFramework.System.Text": "ON"
            }
        }
    ],
    "buildPresets": [
        {
            "inherits": "base-user",
            "name": "ORGPAL_RADAR",
            "displayName": "ORGPAL_RADAR",
            "configurePreset": "ORGPAL_RADAR"
        }
    ]
}
